{
  "name": "kerplunk-blog-social-links",
  "displayName": "Blog Social Links",
  "description": "Add social links to blog",
  "version": "0.0.1",
  "kerplunk": {
    "dependencies": [
      "kerplunk-blog"
    ],
    "permissions": {
      "kerplunk-identity": {
        "models": [
          "Identity"
        ]
      }
    }
  },
  "keywords": [
    "kerplunk-plugin",
    "kp:Blogging"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "lodash": "^3.7.0",
    "react": "^0.13.2"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "glut": "0.0.1",
    "glut-coffee-amdify": "0.0.0",
    "gulp": "^3.8.8",
    "gulp-coffee": "^2.3.1",
    "gulp-stylus": "^2.0.1",
    "gulp-wrap-amd": "^0.5.0"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib ./public && gulp"
  }
}
